{
  "name": "action-wake-up-ec2-runners",
  "version": "0.1.0",
  "private": true,
  "description": "A Github Action to wake up AWS EC2 runners",
  "main": "dist/index.js",
  "engines": {
    "node": "^14.15.0"
  },
  "scripts": {
    "type-check": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build src/main.ts",
    "test": "jest",
    "all": "npm run type-check && npm run format && npm run lint && npm run pack && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MasterworksIO/action-wake-up-ec2-runners.git"
  },
  "keywords": [
    "actions",
    "node",
    "aws",
    "ec2",
    "scaling",
    "runner"
  ],
  "author": "Masterworks",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.9.1",
    "@aws-sdk/client-cloudwatch": "^3.36.1",
    "@aws-sdk/client-ec2": "^3.36.1",
    "loglevel": "^1.7.1"
  },
  "prettier": "@masterworks/eslint-config-masterworks/prettier",
  "devDependencies": {
    "@masterworks/eslint-config-masterworks": "github:MasterworksIO/eslint-config-masterworks#main",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.5",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vercel/ncc": "^0.31.1",
    "eslint": "^8.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^25.0.5",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^27.2.5",
    "jest-circus": "^27.2.5",
    "js-yaml": "^4.0.0",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.4"
  }
}
